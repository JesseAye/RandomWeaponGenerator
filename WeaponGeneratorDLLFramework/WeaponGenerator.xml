    <members>
        <member name="T:WeaponGenerator.Weapon">
            <inheritdoc/>
            <summary>
            An inheritable base class for all weapons
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._clipSize">
            <summary>
            How many rounds a single clip will hold
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._effectiveRange">
            <summary>
            The maximum range a fired round will deliver full effect
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._absMaxRange">
            <summary>
            The maximum range a fired round will deliver negligible, if not zero, effect
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._weight">
            <summary>
            The weight of the weapon
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._reloadTime">
            <summary>
            The averaged TimeSpan it takes to reload a weapon, to be affected by _reloadTimeVariance
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._drawSpeed">
            <summary>
            The amount of time it takes to go from hip-fire to ADS
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._fireRate">
            <summary>
            The minimum TimeSpan between two rounds being fired
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._canDualWield">
            <summary>
            Whether two of this weapon can be held and fired simultaneously
            </summary>
        </member>
        <member name="F:WeaponGenerator.Weapon._isTwoHanded">
            <summary>
            Whether this weapon has to be held by both hands, probably mutually exclusive to _canDualWield
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.WeaponType">
            <summary>
            The enum value of WeaponType the derived class represents
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.LowerClipLimit">
            <summary>
            The lowest value _clipSize can have
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.UpperClipLimit">
            <summary>
            The highest value _clipSize can have
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.LowerEffectiveRangeLimit">
            <summary>
            The lowest value _effectiveRange can have, should be represented as meters
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.UpperEffectiveRangeLimit">
            <summary>
            The highest value _effectiveRange can have, should be represented as meters
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.LowerWeightLimit">
            <summary>
            The lowest value _weight can have, should be represented as grams
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.UpperWeightLimit">
            <summary>
            The highest value _weight can have, should be represented as grams
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.LowerReloadTimeLimit">
            <summary>
            The lowest value _reloadTime can have, should be represented as milliseconds
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.UpperReloadTimeLimit">
            <summary>
            The highest value _reloadTime can have, should be represented as milliseconds
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.LowerFireRateLimit">
            <summary>
            The lowest value _fireRate can have, should be represented as milliseconds
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.UpperFireRateLimit">
            <summary>
            The highest value _fireRate can have, should be represented as milliseconds
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.LowerDrawSpeedLimit">
            <summary>
            The lowest value _drawSpeed can have, should be represented as milliseconds
            </summary>
        </member>
        <member name="P:WeaponGenerator.Weapon.UpperDrawSpeedLimit">
            <summary>
